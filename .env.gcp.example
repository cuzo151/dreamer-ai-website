# Google Cloud Platform Environment Variables
# Copy this file to .env.gcp and fill in your values

# GCP Configuration
GCP_PROJECT_ID=dreamer-ai-website
GCP_REGION=us-central1
GCP_ZONE=us-central1-a
GCP_BILLING_ACCOUNT_ID=YOUR_BILLING_ACCOUNT_ID

# API Keys (store these in Secret Manager for production)
JWT_SECRET=your-secure-jwt-secret
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Database Configuration (auto-generated during deployment)
DB_PASSWORD=will-be-auto-generated

# Domain Configuration
DOMAIN=dreamerai.io

# Service Account Email (auto-generated)
SERVICE_ACCOUNT_EMAIL=dreamer-ai-sa@${GCP_PROJECT_ID}.iam.gserviceaccount.com